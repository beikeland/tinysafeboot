                                 ; accordingly
                                 ;-----------------------------------------------------------------------
                                 ;
                                 ;
                                 .equ	UBRRL	= UBRR0L
                                 .equ	UBRRH	= UBRR0H
                                 .equ	UCSRA	= UCSR0A
                                 .equ	UCSRB	= UCSR0B
                                 .equ	UCSRC	= UCSR0C
                                 
                                 .equ	UDR		= UDR0
                                 
                                 .equ	RXEN	= RXEN0
                                 .equ	TXEN	= TXEN0
                                 .equ	TXC		= TXC0
                                 .equ	RXC		= RXC0
                                 
                                 .equ	UCSZ0	= UCSZ00
                                 .equ	UCSZ1	= UCSZ01
                                 
                                 
