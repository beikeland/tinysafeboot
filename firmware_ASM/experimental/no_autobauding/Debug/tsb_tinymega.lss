000fe4 91a0 00c0                 		lds xl, UCSRA			; load status register
000fe6 ff26                      		sbrs tmp3, TXC			; if TXC is complete (asserted) skip next instruction
000fe7 cffc                      		rjmp CheckTXComplete	; else jump to repeat cycle
000fe8 e1a0                      		ldi xl, (1 << RXEN)		; disable TX, enable RX
000fe9 9320 00c1                 		sts UCSRB, tmp3
