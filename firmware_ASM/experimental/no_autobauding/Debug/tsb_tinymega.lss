                                 TransmitByte:
000fdf e0a8                      		ldi xl, (1 << TXEN)	; enable TX, disable RX
000fe0 93a0 00c1                 		sts UCSRB, xl
000fe2 9300 00c6                 		sts UDR, tmp1
