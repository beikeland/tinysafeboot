000fd4 2722                      		clr tmp3						; inner counter for timeout
                                 
                                 ReceiveByte1:		
000fd5 91a0 00c0                 		lds xl, UCSRA					; load UART status register A
000fd7 fda7                      		sbrc xl, RXC					; if not RXComplete, skip
000fd8 c003                      		rjmp LoadRXByte
