000fc6 fd00                              sbrc tmp1, 0                    ; wait previous SPMEN
000fc7 cffd                              rjmp SPMwait
000fc8 e101                              ldi tmp1, 0b00010001            ; set RWWSRE and SPMEN
000fc9 bf07                              out SPMCSR, tmp1
000fca 95e8                              spm
000fcb 9508                              ret
                                 
